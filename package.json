{
  "name": "hostsbox",
  "version": "1.0.0",
  "description": "基于electron的跨平台hosts管理小工具",
  "main": "src/main.js",
  "scripts": {
    "start": "electron .",
    "package-win": "electron-packager ./ hostsbox-1.0.0 --overwrite --asar --platform=win32 --electron-version=19.0.1 -icon=./src/logo.png --arch=x64 --out=./dist --ignore=.git --ignore=.vscode",
    "package-linux": "electron-packager ./ hostsbox-1.0.0 --overwrite --asar --platform=linux --electron-version=19.0.1 -icon=./src/logo.png --arch=x64 --out=./dist --ignore=.git --ignore=.vscode",
    "package-mac": "",
    "build-pack": "electron-builder --dir",
    "build-dist": "electron-builder"
  },
  "build": {
    "appId": "hostsbox-1.0.0",
    "linux": {
      "target": ["appimage"],
      "icon": "./src/logo.png"
    }
  },
  "author": "lizl6",
  "license": "ISC",
  "devDependencies": {
    "electron": "^19.0.1"
  },
  "dependencies": {
    "electron-store": "^8.0.1",
    "nanoid": "^3.3.4",
    "sqlite3": "^5.0.8",
    "vue": "^3.2.36"
  }
}
